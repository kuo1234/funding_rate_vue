= await axios.get('http://localhost:5000/api/funding_rates');\r\n        this.fundingRates = response.data;\r\n      } catch (error) {\r\n        console.error('Failed to fetch funding rates', error);\r\n      }\r\n    },\r\n    sortBy(key) {\r\n      if (this.sortKey === key) {\r\n        this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';\r\n      } else {\r\n        this.sortKey = key;\r\n        this.sortDirection = 'asc';\r\n      }\r\n    },\r\n    formatRate(rate) {\r\n      return (rate * 100).toFixed(4) + '%';\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchFundingRates();\r\n    this.updateInterval = setInterval(this.fetchFundingRates, 3000); // �C 3 ����s�@��\r\n  },\r\n  beforeUnmount() {\r\n    clearInterval(this.updateInterval);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nth, td {\r\n  border: 1px solid #ccc;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f0f0f0;\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":";;AAqBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC,CAAC;MAChBC,OAAO,EAAE,QAAQ;MACjBC,aAAa,EAAE,KAAK;MACpBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,UAAS,GAAIC,MAAM,CAACC,OAAO,CAAC,IAAI,CAACR,YAAY,CAAC,CAACS,GAAG,CAAC,CAAC,CAACC,MAAM,EAAEC,IAAI,CAAC,MAAM;QAAED,MAAM;QAAEC;MAAK,CAAC,CAAC,CAAC;MAChG,IAAI,IAAI,CAACV,OAAM,KAAM,QAAQ,EAAE;QAC7BK,UAAU,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;UACxB,IAAI,IAAI,CAACZ,aAAY,KAAM,KAAK,EAAE;YAChC,OAAOW,CAAC,CAACH,MAAM,CAACK,aAAa,CAACD,CAAC,CAACJ,MAAM,CAAC;UACzC,OAAO;YACL,OAAOI,CAAC,CAACJ,MAAM,CAACK,aAAa,CAACF,CAAC,CAACH,MAAM,CAAC;UACzC;QACF,CAAC,CAAC;MACJ,OAAO,IAAI,IAAI,CAACT,OAAM,KAAM,MAAM,EAAE;QAClCK,UAAU,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;UACxB,IAAI,IAAI,CAACZ,aAAY,KAAM,KAAK,EAAE;YAChC,OAAOW,CAAC,CAACF,IAAG,GAAIG,CAAC,CAACH,IAAI;UACxB,OAAO;YACL,OAAOG,CAAC,CAA