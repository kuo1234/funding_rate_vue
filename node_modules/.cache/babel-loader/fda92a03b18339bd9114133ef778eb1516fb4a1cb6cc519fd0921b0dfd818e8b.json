{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, computed, onMounted, onUnmounted } from 'vue';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const fundingRates = ref([]);\n    const sortDesc = ref(true);\n    let fetchTimer = null;\n    function rateClass(rate) {\n      return Math.abs(rate) > 0.0005 ? 'high' : '';\n    }\n    function formatRate(rate) {\n      return Number(rate).toFixed(4) + '%';\n    }\n    function toggleSort() {\n      sortDesc.value = !sortDesc.value;\n    }\n    const filteredAndSortedRates = computed(() => {\n      // �u��� abs(rate) > 0.05%\n      const arr = fundingRates.value.filter(item => Math.abs(item.rate) > 0.0005);\n      return arr.sort((a, b) => sortDesc.value ? b.rate - a.rate : a.rate - b.rate);\n    });\n    async function fetchData() {\n      const res = await fetch('http://localhost:5000/api/funding_rates');\n      fundingRates.value = await res.json();\n    }\n    onMounted(async () => {\n      await fetchData();\n      fetchTimer = setInterval(fetchData, 30000); // �C 30 ���۰ʨ�s\n    });\n    onUnmounted(() => {\n      if (fetchTimer) clearInterval(fetchTimer);\n    });\n    const __returned__ = {\n      fundingRates,\n      sortDesc,\n      get fetchTimer() {\n        return fetchTimer;\n      },\n      set fetchTimer(v) {\n        fetchTimer = v;\n      },\n      rateClass,\n      formatRate,\n      toggleSort,\n      filteredAndSortedRates,\n      fetchData,\n      ref,\n      computed,\n      onMounted,\n      onUnmounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","onUnmounted","fundingRates","sortDesc","fetchTimer","rateClass","rate","Math","abs","formatRate","Number","toFixed","toggleSort","valu